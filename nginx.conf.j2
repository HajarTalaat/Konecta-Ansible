server {
    listen 80 default_server;
    listen [::]:80 default_server;

    root /var/www/html;
    index index.html index.htm;
    
    server_name _;

    location / {
        try_files $uri $uri/ =404;
    }

    # Logging Configuration (New Change)
    access_log /var/log/nginx/access.log;
    error_log /var/log/nginx/error.log warn;
}
